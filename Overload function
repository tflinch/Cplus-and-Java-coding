#include <iostream>
using namespace std;

void func(int);
void func(double);

int main()
{
    func(4);
    func(5.6);
    
    system("pause");
    return 0;
}

void func(int a)
{ 
     cout << "passeed an int" << endl;
     }
     
void func(double a)
 {
     cout<< "passed a double"<< endl;
     }       
