#include <iostream>
using namespace std;

void calc(double,double, double&, double&);


int main()
{double a, b, c,d;
    cin >> a >> b;
    calc(a,b, c,d);
    cout <<"product is : " <<c <<"qoutent is : " << d<<endl;  
    system("pause");
    return 0;
}

void calc(double a,double b, double&multi, double&div)
{
     multi=a*b;
     div=a/b;
     }
